{
  "name": "vscode-astx",
  "version": "0.0.1",
  "displayName": "ASTX",
  "description": "structural search and replace for JavaScript and TypeScript",
  "publisher": "jedwards1211",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onView:astx.SearchReplaceView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "astx-astx",
          "title": "Astx",
          "icon": "media/astx.svg"
        }
      ]
    },
    "views": {
      "astx-astx": [
        {
          "type": "webview",
          "id": "astx.SearchReplaceView",
          "name": "Search"
        },
        {
          "id": "astx.MatchesView",
          "name": "Matches"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint $npm_package_config_lint",
    "lint:fix": "eslint $npm_package_config_lint",
    "prettier": "prettier --write $npm_package_config_prettier",
    "prettier:check": "prettier --list-different $npm_package_config_prettier",
    "clean": "rimraf out",
    "check:types": "tsc --noEmit",
    "build": "npm run clean && tsc -p src/extension/tsconfig.json && webpack",
    "test:unit": "mocha --watch-files 'src/**/*' -r ts-node/register src/test/unit/configure.js 'src/test/unit/**/*.ts'",
    "test": "rimraf .nyc_output; npm run test:unit && node ./out/extension/test/runTest.js && nyc report && nyc report --reporter=lcov",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release",
    "build:watch": "nodemon -e js,ts,json --watch src --exec \"(pnpm build)\"",
    "pretest": "BABEL_ENV=coverage npm run build",
    "vscode:prepublish": "npm run clean && npm run prettier:check && npm run lint && npm run test:unit && npm run build"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "vsce package 0.0.1 --no-git-tag-version --out vscode-astx-${nextRelease.version}.vsix"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            "*.vsix"
          ]
        }
      ]
    ],
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "vsce": {
    "dependencies": true,
    "yarn": false
  },
  "config": {
    "lint": "--cache --ext .js,.ts src",
    "prettier": ".babelrc.js *.{json,md} {src,test}/**/*.{js,ts}",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run check:types",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm run test:unit"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,css,md}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codemodsquad/vscode-astx.git"
  },
  "keywords": [
    "search",
    "replace",
    "refactoring",
    "structural-search",
    "semantic-search"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codemodsquad/vscode-astx/issues"
  },
  "homepage": "https://github.com/codemodsquad/vscode-astx#readme",
  "devDependencies": {
    "@babel/node": "^7.19.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jedwards1211/commitlint-config": "^1.0.2",
    "@jedwards1211/eslint-config": "^2.0.2",
    "@jedwards1211/eslint-config-typescript": "^2.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.185",
    "@types/lodash-es": "^4.17.6",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^8.2.0",
    "@types/node": "^16.9.6",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/vscode": "^1.40.0",
    "@types/vscode-webview": "^1.57.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@typescript-eslint/typescript-estree": "^4.32.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "codecov": "^3.8.1",
    "copy": "^0.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "semantic-release-vsce": "^5.1.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vsce": "^2.14.0",
    "vscode-test": "^1.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@repeaterjs/repeater": "^3.0.4",
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "^1.0.1",
    "astx": "^2.0.0-beta.44",
    "babel-parse-wild-code": "^1.2.0",
    "empty": "^0.10.1",
    "gitignore-fs": "^2.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "minimatch": "^5.1.0",
    "p-event": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-typed-emitter": "^2.1.0"
  }
}
